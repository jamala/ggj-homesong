__lua__
max_stress=5000

-- to program future effects,
-- set a value in effects_table
-- where the key is the number of cycles
-- after which the effect should take place
-- and the val is a function effect
-- see play_song() for example
effects_started=false
effects_timer=0
effects_table={}

function _init()
 inside = true
	singing = false
	other_singing = false
	voloff = false
	vollow = false
	stress = 0
	
	notes={}
	
	-- {sprites}, {song}
	songlist =
		--tree song
	{{["sprites"]={30,31}
	 ,["song"]={3,0,1,3,0,1}
	 ,["return_song"]=13
	 }
	}
	
	songs()

	world = {}
	maplocked = true
	debug = {}
	
	go_inside()
	player.x=5*8
	player.y=10*8
	
	topsprites = 
	{
	 {14,63*8,31*8},
	 {15,63*8,28*8}
	}
end

function _update()
 if effects_started then
  effects_timer += 1
 end
 
 if effects_table[effects_timer] then
  effects_table[effects_timer]()
 end
 
	if not voloff then
 	if singing and not vollow then
 		musiclow()
 		vollow=true
 	elseif not singing and vollow then
 		musichigh()
 		vollow=false
 	end
 end
	
	if btn(4) then
 	singing = true
 	
 	if btn(0) then
 		sfx(1,3)
 	end
 	if btn(1) then
 		sfx(2,3)
 	end
 	if btn(2) then
 		sfx(3,3)
 	end
 	if btn(3) then
 		sfx(0,3)
 	end
 	
 	if not other_singing then songs() end
		playerspr.frame = 1
	else
		singing = false
		notes = {}
		match = nil
		move_player()
	end

 updatesing()
	
	if inside then
	 stress = max(0, stress-10)
	else
	 stress += 1
	 if stress == (max_stress/2) then
	  play_outside(46)
	 elseif stress == (3*max_stress/4) then
	  play_outside(28)
	 end
	end

end


function move_player()
	player.vx = 0
	player.vy = 0
	local m = false
	if (btn(0)) then
		 player.vx = -player.spd
		 playerspr.direction = left
		 playerspr.faceleft = true
		 m = true
	end
	if (btn(1)) then
		player.vx = player.spd
		playerspr.direction = right
		playerspr.faceleft = false
		m = true
	end
	if (btn(2)) then
		player.vy = -player.spd
		playerspr.direction = up
		
		m = true
	end
	if (btn(3)) then
		player.vy = player.spd
		playerspr.direction = down
		m = true
	end

	
	if (m) then
		playerspr:update()
		apply_velocity()
	else playerspr.frame = 1 end
	
	
 if inside then
  tileflag=fget(mget(player.x/8,player.y/8))
  if tileflag == 64 then go_outside() end
 else
  tileflag=fget(mget(player.x/8,player.y/8+1))
  if tileflag == 128 then go_inside() end
 end
end

function _draw()
 cls()
 camera()

 if inside then
  map(0,0,0,0,14,14)
 else

  camera(player.x-64,player.y-64)
  map(0,0,0,0,100,100)
 end
 
 camera()
 
 rectfill(80,0,126,4,2)
 rectfill(80,0,(80+((stress/max_stress)*(126-80))),4,12)
 rect(80,0,126,4,12)
 
 -- this is jank, sorry -ng
 if not inside then
  camera(player.x-64,player.y-64)
 end
 
 
 
 --for i = 1,#notes do
 	--print(notes[i], 0, 40+10*i, 12)
 --end

 --draw player
 
	//spr(0,player.x,player.y)
	
	playerspr:draw()
	draw_topsprites()
	drawdebug()
	
	drawsing()
	
end


function drawdebug()
 for i = 1, #debug do
 
 print(debug[i],player.x,player.y-40,10)
 end
 debug = {}

end

function go_inside()
 inside=true
	play_inside(64)
 player.x=10
 player.y=90
end

function go_outside()
 inside=false 
 play_outside(64)
 player.x=528
 player.y=245
end

function isblocked(x,y)
	
	y+=8 //offset to lower body
	local xmax = (((x+8)-((x+8) % 8) ) / 8)
	local ymax = (((y+8)-((y+8) % 8) ) / 8)
 
 x =((x-(x % 8) ) / 8)
 y = ((y-(y % 8) ) / 8)
		
 local f = 
 (fget(mget(x,y),0)) or
 (fget(mget(xmax,y),0)) or
 (fget(mget(x,ymax),0)) or
 (fget(mget(xmax,ymax),0))
	return f

end


function apply_velocity()
	local b = isblocked(player.x+player.vx,player.y)
 local mx = false
 local my = false
	if not b then
		player.x += player.vx
		mx = true
 end
 
	b = isblocked(player.x,player.y+player.vy)

	if not b then
	 my = true
		player.y += player.vy
	end
	
	if (not mx) player.vx = 0
	if (not my) player.vy = 0
end


function draw_topsprites()

	for t=1, #topsprites do
		spr(topsprites[t][1],
					 topsprites[t][2]+player.vx,
					 topsprites[t][3]+player.vy)		
	end	

end


