sing_note = 
{
 x = 0,
 y = 0,
 vel = 1,
 bounds = {10,10},
 start = 0
}
singduration = 0.75
sing_note_arr = {}
notecols = {10,9,8,2}
function sing(col)

	s = copy(sing_note)
	s.x = (rnd(5)-2.5)+player.x
	s.y = player.y
	s.vel = (rnd(2)-1)
	s.col = notecols[col+1]
	s.bounds = {s.x-10,s.x+10}
	s.start = time()

	add(sing_note_arr,s)
end

function sing_note:update()

	self.y -=1
	if( self.x < self.bounds[1] )
	or
	( self.x > self.bounds[2] )
	then
		self.vel = -self.vel
	end
	
	self.x +=self.vel
	


	
end

function sing_note:draw()
	circ(self.x,self.y,1,self.col)
end

function updatesing()
	for i = 1, #sing_note_arr do
		sing_note_arr[i]:update()
	end
	for i = 1, #sing_note_arr do
		if (sing_note_arr[i].start) < 
		(time() - singduration) then
			del(sing_note_arr,sing_note_arr[i])
			break
	end
end

end

function drawsing()
	for i = 1, #sing_note_arr do
		sing_note_arr[i]:draw()
	end
end

